{
  "engines": {
    "node": "8.4.0"
  },
  "name": "smartsheet-todo-backend",
  "version": "0.0.1",
  "description": "Backend for the Todo app using Smartsheet",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node index.js",
    "watch": "nodemon index.js",
    "test-watch": "jest --watch -i --coverage",
    "test": "jest -i --coverage",
    "dbonWin": "mkdir -p $HOME/db && mongod --dbpath $HOME/db",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db",
    "dboff": "killall mongod"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "http-errors": "^1.6.2",
    "jsonwebtoken": "^8.1.1",
    "morgan": "^1.9.0",
    "smartsheet": "^1.0.4",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^4.18.0",
    "faker": "^4.1.0",
    "jest": "^22.3.0",
    "nodemon": "^1.15.0"
  }
}
